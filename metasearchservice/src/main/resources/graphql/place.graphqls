input ChoosePlaceRequest{
    orderId: ID!
    lastName: String!
    firstName: String!
    passport:Int!
    place:Int!
}
input ChoosePlaceDtoRequest{
    idPath: ID
    login: String
    places:[ChoosePlaceRequest]
}

type ChoosePlaceResponse{
    orderId: ID
    ticket: String
    firstName: String
    lastName: String
    place:Int
}
type ChoosePlacesResponse{
    placeResponses:[ChoosePlaceResponse]
}


